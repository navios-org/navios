{
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    }
  },
  "affected": {
    "defaultBase": "main"
  },
  "plugins": ["@nx/vite/plugin"],
  "generators": {
    "@nx/angular:application": {
      "style": "none",
      "linter": "none",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nx/angular:library": {
      "linter": "none",
      "unitTestRunner": "none"
    },
    "@nx/angular:component": {
      "style": "none"
    }
  }
}
