{
  "name": "@navios/builder",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Oleksandr Hanzha",
    "email": "alex@granted.name"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Arilas/navios.git",
    "directory": "packages/builder"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "typings": "./lib/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    },
    "./socket": {
      "import": {
        "types": "./lib/socket.d.mts",
        "default": "./lib/socket.mjs"
      },
      "require": {
        "types": "./lib/socket.d.cts",
        "default": "./lib/socket.cjs"
      }
    },
    "./eventsource": {
      "import": {
        "types": "./lib/eventsource.d.mts",
        "default": "./lib/eventsource.mjs"
      },
      "require": {
        "types": "./lib/eventsource.d.cts",
        "default": "./lib/eventsource.cjs"
      }
    }
  },
  "scripts": {
    "check": "tsc -b",
    "lint": "yarn run -T oxlint --fix && yarn run -T oxfmt",
    "test:ci": "vitest run",
    "build": "tsdown",
    "publish": "yarn npm publish --access public",
    "publish:next": "yarn npm publish --access public --tag next"
  },
  "devDependencies": {
    "@navios/http": "workspace:^",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  }
}
