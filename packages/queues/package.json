{
  "name": "@navios/queues",
  "version": "0.1.0",
  "description": "Queue adapters for Navios - RabbitMQ, Kafka, and AWS SQS/SNS support",
  "license": "MIT",
  "author": {
    "name": "Oleksandr Hanzha",
    "email": "alex@granted.name"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Arilas/navios.git",
    "directory": "packages/queues"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "typings": "./lib/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "scripts": {
    "check": "tsc -b",
    "lint": "yarn run -T oxlint --fix && yarn run -T oxfmt",
    "test:ci": "vitest run",
    "build": "tsdown",
    "publish": "yarn npm publish --access public",
    "publish:next": "yarn npm publish --access public --tag next"
  },
  "devDependencies": {
    "@aws-sdk/client-sns": "^3.971.0",
    "@aws-sdk/client-sqs": "^3.971.0",
    "@navios/di": "workspace:^",
    "@types/amqplib": "^0.10.8",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "kafkajs": "^2.2.4",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "@navios/di": "workspace:^",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-sns": {
      "optional": true
    },
    "@aws-sdk/client-sqs": {
      "optional": true
    },
    "amqp-connection-manager": {
      "optional": true
    },
    "amqplib": {
      "optional": true
    },
    "kafkajs": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@aws-sdk/client-sns": "^3.971.0",
    "@aws-sdk/client-sqs": "^3.971.0",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "kafkajs": "^2.2.4"
  }
}
