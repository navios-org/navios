  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Arilas/navios.git",
    "directory": "packages/adapter-bun"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "typings": "./lib/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "scripts": {
    "check": "tsc -b",
    "lint": "yarn run -T oxlint --fix && yarn run -T oxfmt",
    "test:ci": "bun test",
    "build": "tsdown",
    "publish": "yarn npm publish --access public",
    "publish:next": "yarn npm publish --access public --tag next"
  },
  "devDependencies": {
    "@navios/builder": "workspace:^",
    "@navios/core": "workspace:^",
    "@types/bun": "^1.3.6",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@navios/builder": "workspace:^",
    "@navios/core": "workspace:^"
  }
}
